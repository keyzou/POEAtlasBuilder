{
  "name": "poe-atlas-builder",
  "version": "0.1.0",
  "license": "Apache License 2.0",
  "main": "main/index.js",
  "private": true,
  "author": {
    "name": "Keyzou"
  },
  "description": "Path of Exile - Atlas Builder",
  "keywords": [
    "path",
    "of",
    "exile",
    "poe",
    "atlas",
    "planner",
    "builder"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/keyzou/poe-atlas-builder.git"
  },
  "productName": "poe-atlas-builder",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "deploy": "npm run build && electron-builder --win --publish always",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@pixi/graphics-smooth": "^0.0.20",
    "@vitejs/plugin-react": "^1.1.3",
    "comlink": "^4.3.1",
    "dexie": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.1",
    "pixi-viewport": "^4.34.4",
    "pixi.js": "^6.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-string-replace": "^0.4.4",
    "use-callback-ref": "^1.2.5",
    "vite": "^2.7.2",
    "vite-plugin-comlink": "^2.1.0",
    "tailwind-scrollbar": "^1.3.1",
    "vite-plugin-worker": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^16.11.13",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.5",
    "typescript": "^4.5.4"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
